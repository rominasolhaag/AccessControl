import com.example.CRUD.modelos.Usuario;
import java.util.list;

public interface IUsuarioBD{
    Usuario elemento (int nro_dni); //devuelve el elemnto dado su nro_dni
    Usuario elemento (String nombre);
    Usuario elemento (String apellido)

    List<Usuario> lista();//devuelve una lista con todos los elemnetos registrados

    void agregar(Usuario); //agrega el elemento dado
    void actualizar(int nro_dni, Usuario); //actualiza el elemento dado su nro_dni
    void eliminar(int nro_dni); //elimina el elemento dado su nro_dni
}